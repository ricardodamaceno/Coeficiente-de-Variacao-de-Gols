package com.apostas.model;

public class Time {

	private String time;

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public Time(String time) {
		this.time = time;
	}
	
	public Time() {
	}
	
	public String setaTime(String nomeTime){
		nomeTime = this.time;
		return nomeTime;
	}
}
